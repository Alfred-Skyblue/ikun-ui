{
	"name": "ikun-ui",
	"description": "",
	"type": "module",
	"version": "0.0.2",
	"keywords": [
		"web component",
		"component",
		"react",
		"vue",
		"svelte",
		"svelte-kit",
		"dx"
	],
	"license": "MIT",
	"author": "baiwusanyu-c",
	"homepage": "https://github.com/baiwusanyu-c/ikun-ui#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/baiwusanyu-c/ikun-ui.git"
	},
	"bugs": {
		"url": "https://github.com/baiwusanyu-c/ikun-ui/issues"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": "./*"
	},
	"files": [
		"dist",
		"*.d.ts"
	],
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"init": "pnpm i",
		"dev": "pnpm run build",
		"dev:docs": "",
		"dev:playground": "",
		"play": "pnpm run --filter @ikun-ui/play dev",
		"build": "pnpm run build:preset && pnpm run build:components",
		"build:docs": "",
		"build:preset": "pnpm run --filter @ikun-ui/preset build",
		"build:components": "esno scripts/build.js",
		"build:playground": "",
		"test": "npm run test:integration && npm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir --check . && eslint .",
		"format": "prettier --plugin-search-dir --write .",
		"release": "bumpp package.json components/*/package.json preset/package.json --commit --push --tag",
		"publish:script": "esno scripts/publish.js",
		"publish:npm": "pnpm publish --no-git-checks --access public",
		"clean:dist": "rimraf dist && esno scripts/clean.js",
		"clean:deps": "esno scripts/clean-deps.js && node scripts/clean-root-deps.js",
		"prepare": "npx simple-git-hooks",
		"update:deps": "npx taze -w -r major && pnpm run init"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"dependencies": {
		"@ikun-ui/button": "workspace:*",
		"@ikun-ui/callapse": "workspace:*",
		"@ikun-ui/checkbox": "workspace:*",
		"@ikun-ui/drawer": "workspace:*",
		"@ikun-ui/eyeDropper": "workspace:*",
		"@ikun-ui/icon": "workspace:*",
		"@ikun-ui/input": "workspace:*",
		"@ikun-ui/mask": "workspace:*",
		"@ikun-ui/message": "workspace:*",
		"@ikun-ui/modal": "workspace:*",
		"@ikun-ui/clientOnly": "workspace:*",
		"@ikun-ui/notify": "workspace:*",
		"@ikun-ui/popover": "workspace:*",
		"@ikun-ui/select": "workspace:*",
		"@ikun-ui/switch": "workspace:*",
		"@ikun-ui/tag": "workspace:*",
		"@ikun-ui/tooltip": "workspace:*",
		"@ikun-ui/utils": "workspace:*",
		"@ikun-ui/preset": "workspace:*"
	},
	"devDependencies": {
		"@playwright/test": "^1.36.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.3",
		"@sveltejs/package": "^2.2.0",
		"@sveltejs/vite-plugin-svelte": "latest",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"@unocss/preset-mini": "^0.53.6",
		"@unocss/preset-uno": "^0.53.6",
		"baiwusanyu-utils": "^1.0.14",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.4",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.1",
		"publint": "^0.1.16",
		"rimraf": "^5.0.1",
		"simple-git-hooks": "^2.9.0",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "latest",
		"tslib": "^2.6.1",
		"tsup": "^6.7.0",
		"typescript": "^5.1.6",
		"unocss": "^0.53.6",
		"vite": "^4.4.7",
		"vitest": "^0.33.0",
		"shelljs": "^0.8.5",
		"esno": "^0.16.3",
		"del": "^7.0.0",
		"fast-glob": "^3.2.12"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm run format && pnpm run lint",
		"commit-msg": "node vertify-commit.js"
	}
}
